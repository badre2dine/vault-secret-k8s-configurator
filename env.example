# Example .env file for local development
# Copy this to .env and fill with your actual values
# Format: KEY=VALUE
#
# NOTE: When deploying with deploy.sh, only the KEYS will be used
# Values will be created as EMPTY in Vault for security
# You must fill the values manually in Vault after deployment

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=myapp_db
DB_USER=dbuser
DB_PASSWORD=local_password

# API Keys
API_KEY=your_api_key_here
API_SECRET=your_api_secret_here
BINANCE_API_KEY=your_binance_key
BINANCE_SECRET_KEY=your_binance_secret

# Application Config
APP_ENV=development
APP_DEBUG=true
APP_SECRET_KEY=local_secret_key
JWT_SECRET=local_jwt_secret
JWT_EXPIRATION=3600

# External Services
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=redis_pass
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_smtp_password

# AWS Credentials
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
AWS_REGION=us-east-1

# Custom Variables
CUSTOM_VAR1=value1
CUSTOM_VAR2=value2
